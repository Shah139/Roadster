{
    "version": "0.2.0",
    "configurations": [
        

        {
            "type": "java",
            "name": "Launch Roadster JavaFX App",
            "request": "launch",
            "mainClass": "com.roadster.Main",
            "projectName": "roadster-demo",
            "vmArgs": [
                "--module-path",
                "${workspaceFolder}/target/dependency",
                "--add-modules",
                "javafx.controls,javafx.fxml,javafx.graphics,javafx.base"
            ],
            "preLaunchTask": "mvn-compile",
            "console": "integratedTerminal"
        },
        {
            "type": "java",
            "name": "Debug Roadster JavaFX App",
            "request": "launch",
            "mainClass": "com.roadster.Main",
            "projectName": "roadster-demo",
            "vmArgs": [
                "--module-path",
                "${workspaceFolder}/target/dependency",
                "--add-modules",
                "javafx.controls,javafx.fxml,javafx.graphics,javafx.base"
            ],
            "preLaunchTask": "mvn-compile",
            "console": "integratedTerminal"
        }
    ]
} 